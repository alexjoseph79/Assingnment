#include <stdio.h>

int stack[MAX];
int top = -1;

void push(int value) {
    if (top == MAX - 1){
        printf("stack is overflow");
    } else{
        top++;
        stack[top] = value;
        printf("%d push into stack\n", value)
    }
}
 void display(){
     if (top == -1){
         printf("stack is empty\n");
     } else
     printf("stack elements are: \n");
     for(int i = top; i>=0 i--){
         printf("%d\n" stack)
     }
 }
